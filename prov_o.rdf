@prefix ex: <http://example.org/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:activity_1 a prov:Activity ;
    prov:endedAtTime "2024-01-01T12:00:00"^^xsd:dateTime ;
    prov:startedAtTime "2023-01-01T12:00:00"^^xsd:dateTime ;
    prov:used ex:entity1 ;
    prov:wasAssociatedWith ex:agent1 .

ex:entity1 a prov:Entity ;
    ex:description "This is an example entity."^^xsd:string ;
    ex:name "Example Entity"^^xsd:string ;
    ex:value "42"^^xsd:int ;
    prov:wasAttributedTo ex:agent1 ;
    prov:wasGeneratedBy ex:activity_1 .

ex:agent1 a prov:Agent,
        prov:Person ;
    ex:name "007"^^xsd:string .

